{
  "expo": {
    "name": "ecodive-osrm",
    "slug": "ecodive-osrm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./res/icon.png",
    "splash": {
      "image": "./res/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./res/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./res/favicon.png"
    }
  }
}
